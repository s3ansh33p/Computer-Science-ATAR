{
    "title": "Computer Science Project",
    "name": "computer-science-atar",
    "version": "0.0.1",
    "scripts": {
        "build": "npm run clean && npm run build:scss && npm run build:scripts && npm run build:assets && npm run build:docs && npm run build:main",
        "build:assets": "node scripts/build-assets.js",
        "build:docs": "jsdoc src -r -d ./dist/docs",
        "build:main": "node scripts/build-main.js",
        "build:prod": "node scripts/build-prod.js",
        "build:scripts": "node scripts/build-scripts.js",
        "build:scss": "node scripts/build-scss.js",
        "clean": "node scripts/clean.js",
        "start": "npm run build && node scripts/start.js",
        "start:debug": "node scripts/start.js"
    },
    "description": "A game for the Computer Science ATAR course",
    "keywords": [
        "game",
        "three.js"
    ],
    "homepage": "https://dev.seanmcginty.space",
    "license": "MIT",
