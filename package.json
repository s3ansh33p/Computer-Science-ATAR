        "crypto": "^1.0.1",
        "debug": "^4.3.1",
        "dotenv": "^10.0.0",
        "ejs": "^3.1.6",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "jsdoc": "^3.6.7",
        "mysql": "^2.18.1",
        "three": "^0.129.0",
        "ws": "^7.4.6"
    },
    "devDependencies": {
        "autoprefixer": "10.2.6",
        "browser-sync": "2.26.14",
        "chokidar": "3.5.1",
        "concurrently": "6.2.0",
        "postcss": "8.3.0",
        "prettier": "2.3.0",
        "sass": "1.34.1",
        "shelljs": "0.8.4",
        "upath": "2.0.1"
    }
